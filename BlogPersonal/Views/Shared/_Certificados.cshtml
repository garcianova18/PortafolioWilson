@model IEnumerable<DTOs.DTO.Certificaciones>




    


        <div class="row">

            <h2 class="text-center text-primary my-5">Certificaciones</h2>

            @foreach (var item in Model)
            {
                <div class="col-12 col-md-6 col-lg-4 mb-3">

                    <div class="card text-center shadow bg-dark p-3 text-white">

                        <img src="~/Imagenes/Certificados/@item.Foto" alt="..." height="200" class="rounded-2 img-certificados">

                        <div class="card-body">

                            <h6 class="card-title ">@item.Titulo</h6>

                            <small class="text-muted" >@item.Centro</small>

                           
                        </div>
                    </div>

                </div>
            }


        </div>


